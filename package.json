{
  "name": "travelbliss",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "preview:test": "vite preview --outDir ./generated/test",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm eslint && pnpm stylelint",
    "eslint": "eslint . --cache",
    "stylelint": "stylelint \"**/*.css\" --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "format": "pnpm prettier:write && pnpm eslint --fix && pnpm stylelint --fix",
    "vitest": "CI=true vitest run",
    "vitest:watch": "CI=true vitest",
    "test": "CI=true pnpm typecheck && pnpm prettier && pnpm lint && pnpm vitest && pnpm build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "prepare": "husky",
    "codegen": "graphql-codegen --config codegen.ts",
    "postinstall": "pnpm codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@mantine/core": "^7.17.3",
    "@mantine/dates": "^7.17.3",
    "@mantine/form": "^7.17.3",
    "@mantine/hooks": "^7.17.3",
    "@storybook/preview-api": "^8.6.8",
    "@tabler/icons-react": "^3.31.0",
    "dayjs": "^1.11.13",
    "graphql": "^16.10.0",
    "lodash": "^4.17.21",
    "mantine-form-zod-resolver": "^1.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.4.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/js": "^9.24.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.7.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@storybook/addon-essentials": "^8.6.8",
    "@storybook/addon-interactions": "^8.6.8",
    "@storybook/addon-links": "^8.6.8",
    "@storybook/blocks": "^8.6.8",
    "@storybook/react": "^8.6.8",
    "@storybook/react-vite": "^8.6.8",
    "@storybook/test": "^8.6.8",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.17.30",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.9",
    "@vitest/ui": "^3.0.9",
    "eslint": "^9.24.0",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.3",
    "postcss-preset-mantine": "1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.5.3",
    "prop-types": "^15.8.1",
    "storybook": "^8.6.8",
    "storybook-dark-mode": "^4.0.2",
    "stylelint": "^16.16.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
